{
	"name": "rental-monitor",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"rr:generate": "react-router typegen",
		"cf-typegen": "wrangler types",
		"deploy": "pnpm run build && wrangler deploy",
		"dev": "react-router dev",
		"postinstall": "npm run cf-typegen",
		"preview": "pnpm run build && vite preview",
		"typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
		"data:generate-regions-json": "bun app/scripts/generate-regions-json.ts > ./app/scripts/data/regions.json",
		"data:generate-regions": "bun app/scripts/generate-regions.ts > ./app/scripts/data/regions.sql",
		"data:import-regions:local": "pnpm run data:generate-regions && wrangler d1 execute rental_monitor --file ./app/scripts/data/regions.sql --local",
		"data:import-regions:remote": "pnpm run data:generate-regions && wrangler d1 execute rental_monitor --file ./app/scripts/data/regions.sql --remote",
		"data:generate-listings": "bun app/scripts/generate-listings-sql.ts > ./app/scripts/data/listings.sql",
		"data:import-listings:local": "pnpm run data:generate-listings && wrangler d1 execute rental_monitor --file ./app/scripts/data/listings.sql --local",
		"data:import-listings:remote": "pnpm run data:generate-listings && wrangler d1 execute rental_monitor --file ./app/scripts/data/listings.sql --remote",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply rental_monitor --local",
		"db:migrate:remote": "wrangler d1 migrations apply rental_monitor --remote",
		"db:studio": "drizzle-kit studio",
		"db:studio:remote": "NODE_ENV=production drizzle-kit studio",
		"db:import:remote-to-local": "bun app/scripts/db-import-remote-to-local.ts",
		"db:import:local-to-remote": "bun app/scripts/db-import-local-to-remote.ts",
		"cron:deploy": "wrangler deploy --config workers/cron-scraper/wrangler.jsonc",
		"cron:dev": "wrangler dev --config workers/cron-scraper/wrangler.jsonc",
		"cron:discovery": "curl \"http://localhost:8787/cdn-cgi/handler/scheduled?cron=%2A%2F30+%2A+%2A+%2A+%2A\"",
		"cron:sweep": "curl \"http://localhost:8787/cdn-cgi/handler/scheduled?cron=0+%2A%2F3+%2A+%2A+%2A\"",
		"cron:verification": "curl \"http://localhost:8787/cdn-cgi/handler/scheduled?cron=0+%2A%2F6+%2A+%2A+%2A\"",
		"cron:test": "vitest run",
		"cron:test:discovery": "vitest run workers/cron-scraper/sources/willhaben/overview.test.ts",
		"cron:test:sweep": "vitest run workers/cron-scraper/runs/sweep.test.ts",
		"cron:test:verification": "vitest run workers/cron-scraper/sources/willhaben/detail.test.ts",
		"cron:db:migrate:local": "wrangler d1 migrations apply rental_monitor --local --config workers/cron-scraper/wrangler.jsonc",
		"cron:db:copy-local": "mkdir -p workers/cron-scraper/.wrangler/state/v3/d1/miniflare-D1DatabaseObject && cp .wrangler/state/v3/d1/miniflare-D1DatabaseObject/*.sqlite workers/cron-scraper/.wrangler/state/v3/d1/miniflare-D1DatabaseObject/"
	},
	"dependencies": {
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@types/geojson": "^7946.0.16",
		"chalk": "^5.6.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.7",
		"isbot": "^5.1.31",
		"leaflet": "^1.9.4",
		"lucide-react": "^0.548.0",
		"motion": "^12.23.24",
		"ora": "^9.0.0",
		"pretty-cache-header": "^1.0.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-leaflet": "^5.0.0",
		"react-router": "^7.9.2",
		"recharts": "2.15.4",
		"tailwind-merge": "^3.3.1",
		"vaul": "^1.1.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.13.5",
		"@react-router/dev": "^7.9.2",
		"@tailwindcss/vite": "^4.1.13",
		"@types/leaflet": "^1.9.21",
		"@types/node": "^22",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.31.5",
		"tailwind-scrollbar": "^4.0.2",
		"tailwindcss": "^4.1.13",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.6",
		"wrangler": "^4.45.1"
	}
}